{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "pnpm dlx @biomejs/biome format .",
    "format:fix": "pnpm dlx @biomejs/biome format . --write",
    "lint": "pnpm dlx @biomejs/biome lint .",
    "lint:fix": "pnpm dlx @biomejs/biome lint --apply .",
    "check": "pnpm dlx @biomejs/biome check .",
    "check:fix": "pnpm dlx @biomejs/biome check --apply .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@repo/typescript-config": "workspace:*",
    "husky": "^9.0.11",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
